<h5>商品新規登録</h5>

<div class="form-group">
  <%= f.label "商品画像" %>
  <%= f.file_field :image, accept: "image/*" %>
</div>

<div class="form-group">
  <%= f.label "商品名"%>
  <%= f.text_field :title%>
</div>

<div class="form-group">
  <%= f.label "商品の説明" %>
  <%= f.text_area :description, class: 'form-control' %>
</div>

<div class="form-group">
　<%= f.label "税抜き価格" %>
  <%= f.number_field :price%>
</div>

<div class="actions">
  <button type="submit" class="btn btn-success">新規登録</button>
</div>