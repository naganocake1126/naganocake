<div class="container">

    <td>商品一覧</td>

 <table class="table">
  <thead class="table-info">

    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜価格</th>
    </tr>
  </thead>
  <tbody>
    <% @item.each do |item| %>
    <tr>
      <td><%= item.id %></td>

      <td><%= link_to item.name, admin_item_path(item) %></td>

      <td><%= item.price %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @item %>

</div>








