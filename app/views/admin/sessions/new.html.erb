<div class="container">
<h5>管理者ログイン</h5>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
   <table>
    <tr>
     <td><%= f.label :メールアドレス %><br /></td>
     <td><%= f.email_field :email, autofocus: true, autocomplete: "email" %></td>
    </tr>
  </div>

  <div class="field">
  　<tr>
     <td><%= f.label :パスワード %><br /></td>
     <td><%= f.password_field :password, autocomplete: "current-password" %></td>
    </tr>
  </div>

   </table>
   


  <div class="actions">
  <input class="btn btn-outline-primary my-1" type="submit" value="ログイン"/>
  </div>

<% end %>

<%= render "admin/shared/links" %>
</div>
